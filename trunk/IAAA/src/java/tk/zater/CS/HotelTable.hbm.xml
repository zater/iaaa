<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


  

<hibernate-mapping package="tk.zater.CS">

    
    <class name="HotelTable">
    	<id name="id">
    		<generator class="native"/>
    	</id>
        <property name="memoId" not-null="true" length="10"/>
        
    	<property name="hotelName" not-null="true" length="10"/>
    	<property name="hotelTel" not-null="false" length="20"/>
        <property name="hotelAddress" not-null="false" length="20"/>
    	<property name="htotelRemark" not-null="false" length="50"/>
    </class>

</hibernate-mapping>
