<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


  

<hibernate-mapping package="tk.zater.CS">

    <class name="PointTable">
    	<id name="id">
    		<generator class="native"/>
    	</id>
    	<property name="memoId" not-null="true" length="10"/>
    	<property name="pointName" not-null="true" length="20"/>
        <property name="pointSummary" not-null="false" length="2000"/>
        <property name="pointNum" not-null="true" length="10"/>
       
    </class>

</hibernate-mapping>
