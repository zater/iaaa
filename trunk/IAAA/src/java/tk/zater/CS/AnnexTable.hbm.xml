<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="tk.zater.CS">

    <class name="AnnexTable">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="planId" not-null="true" length="10"/>
        <property name="memoId" not-null="false" length="20"/>
        <property name="foodId" not-null="false" length="10"/
        <property name="hotelId" not-null="false" length="10"/>
        <property name="annexURL" not-null="true" length="50"/>
    </class>

</hibernate-mapping>
