<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


  

<hibernate-mapping package="tk.zater.CS">

    
    
    <class name="MemoTable">
    	<id name="id">
    		<generator class="native"/>
    	</id>
    	<property name="day" not-null="true" length="10"/>
        <property name="planId" not-null="true" length="10"/>
    	<property name="memo" not-null="false" length="200"/>
        <property name="memoTraffic" not-null="false" type="text"/>
    </class>

</hibernate-mapping>
