<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">





<hibernate-mapping package="tk.zater.CS">

    <class name="UserTable">
    	<id name="id">
    		<generator class="native"/>
    	</id>
    	<property name="accountName" not-null="true" length="15"/>
    	<property name="pwd" not-null="true" length="20"/>
        <property name="userName" not-null="true" length="20"/>
        <property name="email" not-null="true" length="20"/>
        <property name="addDate" type="date"/>
        <property name="address" not-null="false" length="40" />
        <property name="tel" not-null="false" length="10"/>
        <property name="inrtoduction" not-null="false" length="100"/>
        <property name="userLv" not-null="true" length="10" />
        <property name="userScore" not-null="true" length="10"/>
    </class>
 
</hibernate-mapping>
