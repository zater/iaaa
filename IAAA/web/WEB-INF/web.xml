<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CreateHibernateServer</servlet-name>
        <servlet-class>tk.zater.CreateSession.CreateHibernateServer</servlet-class>
        <load-on-startup>0</load-on-startup>
        <init-param>
            <param-name>EnableFileUpload</param-name>
            <param-value>jpg,png,gif,pdf</param-value>
        </init-param>
        <init-param>
            <param-name>uploadpath</param-name>
            <param-value>upload</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>reg</servlet-name>
        <servlet-class>tk.zater.Servlet.reg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>tk.zater.Servlet.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pictureUpload</servlet-name>
        <servlet-class>tk.zater.Servlet.pictureUpload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserIsExist</servlet-name>
        <servlet-class>tk.zater.Servlet.UserIsExist</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>json</servlet-name>
        <servlet-class>tk.zater.Servlet.json</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GainInfo</servlet-name>
        <servlet-class>tk.zater.json.GainInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addTravel</servlet-name>
        <servlet-class>tk.zater.Servlet.addTravel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>tk.zater.json.Search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Gainplantype</servlet-name>
        <servlet-class>tk.zater.json.Gainplantype</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlanType</servlet-name>
        <servlet-class>tk.zater.Servlet.PlanType</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateHibernateServer</servlet-name>
        <url-pattern>/CreateHibernateServer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reg</servlet-name>
        <url-pattern>/reg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pictureUpload</servlet-name>
        <url-pattern>/pictureUpload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserIsExist</servlet-name>
        <url-pattern>/UserIsExist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>json</servlet-name>
        <url-pattern>/json</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GainInfo</servlet-name>
        <url-pattern>/GainInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addTravel</servlet-name>
        <url-pattern>/addTravel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/Search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Gainplantype</servlet-name>
        <url-pattern>/Gainplantype</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlanType</servlet-name>
        <url-pattern>/PlanType</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
